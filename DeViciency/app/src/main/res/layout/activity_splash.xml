<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".Splash">
	<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" app:srcCompat="@drawable/p0" android:id="@+id/animate"
			app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
			android:layout_marginBottom="32dp"/>
	<TextView
			android:text="@string/app_name" android:textSize="30sp"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/textView2"
			app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="@+id/animate" app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintVertical_bias="0.71"/>
</androidx.constraintlayout.widget.ConstraintLayout>